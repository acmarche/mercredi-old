{% set class = '' %}
{% if  message.error %}
    {% set class = 'danger' %}
{% else %}
    {% set class = 'success' %}
{% endif %}
<div class="panel panel-{{ class }}">    
    <div class="panel-heading">
        <h3 class="panel-title">Message</h3>
    </div>
    <div class="panel-body">

        {{ message.message }}

    </div>
</div>